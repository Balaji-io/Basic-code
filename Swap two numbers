//Regular Model

#include <stdio.h>

int main() {
    int a, b;
    scanf("%d %d", &a, &b); 

    // Swapping the values
    int temp = a;
    a = b;
    b = temp;

    // Printing the swapped values
    printf("%d %d", a, b);

    return 0;
}
------------------------------------------------------------------------------------------
//with using fn

#include <stdio.h>

void swap(int *a, int *b) {
    int temp = *a;  
    *a = *b;     
    *b = temp;     
}

int main() {
    int a, b;
    
   
    scanf("%d %d", &a, &b);
    
    // Call swap function with the addresses of a and b
    swap(&a, &b);
    
    // Print the swapped values
    printf("%d %d", a, b);
    
    return 0;
}


